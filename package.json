{
  "name": "booking-frontend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">6.0.0"
  },
  "scripts": {
    "test": "xo && mocha src/test"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "date-fns": "^1.28.2",
    "express": "^4.15.2",
    "got": "^6.7.1",
    "govuk-elements-marko": "^4.0.0",
    "govuk-elements-sass": "^3.0.2",
    "govuk-template-marko": "^1.0.0",
    "helmet": "^3.5.0",
    "i18next": "^7.1.3",
    "i18next-express-middleware": "^1.0.3",
    "i18next-node-fs-backend": "^0.1.3",
    "jquery": "^1.12.4",
    "lasso": "~2.11.8",
    "lasso-sass": "^2.0.0",
    "marko": "^4.2.7",
    "marko-i18next": "^0.2.0",
    "nodemailer": "^4.0.0",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "mocha": "^3.2.0",
    "xo": "^0.18.1"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "env": [
      "node",
      "mocha",
      "browser",
      "jquery"
    ],
    "ignores": [
      "**/*.marko.js"
    ],
    "overrides": [
      {
        "files": "src/common/application.js",
        "esnext": false
      }
    ],
    "rules": {
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
