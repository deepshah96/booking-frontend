{
  "name": "booking-frontend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">6.0.0"
  },
  "scripts": {
    "test": "xo"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "date-fns": "^1.29.0",
    "express": "^4.16.2",
    "gmail-node": "^1.2.0",
    "got": "^7.1.0",
    "govuk-elements-marko": "^4.2.1",
    "govuk-elements-sass": "^3.1.2",
    "govuk-template-marko": "^2.0.3",
    "govuk_frontend_toolkit": "^7.1.0",
    "helmet": "^3.9.0",
    "i18next": "^10.0.7",
    "i18next-express-middleware": "^1.0.7",
    "i18next-node-fs-backend": "^1.0.0",
    "lasso": "~2.11.21",
    "lasso-sass": "^3.0.0",
    "marko": "^4.5.6",
    "marko-i18next": "^0.2.1",
    "shrink-ray": "^0.1.3",
    "validator": "^9.1.1"
  },
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "xo": {
    "space": true,
    "env": [
      "node",
      "browser",
      "jquery"
    ],
    "ignores": [
      "./**/*.marko.js"
    ],
    "overrides": [
      {
        "files": "src/common/application.js",
        "esnext": false
      }
    ],
    "rules": {
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
